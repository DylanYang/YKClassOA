<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yk.model.StuFamilyEntity" table="yk_stu_family">

        <id name="id" column="id">
            <generator class="native"></generator>
        </id>
        <property name="stu_id" column="stu_id" type="long"></property>
        <property name="father_name" column="father_name" type="java.lang.String"></property>
        <property name="father_tel" column="father_tel" type="long"></property>
        <property name="father_id_card" column="father_id_card" type="java.lang.String"></property>
        <property name="mother_name" column="mother_name" type="java.lang.String"></property>
        <property name="mother_tel" column="mother_tel" type="long"></property>
        <property name="mother_id_card" column="mother_id_card" type="java.lang.String"></property>
        <property name="family_addr" column="family_addr" type="java.lang.String"></property>
    </class>
</hibernate-mapping>